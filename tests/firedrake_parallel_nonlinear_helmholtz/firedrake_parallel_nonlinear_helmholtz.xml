<?xml version='1.0' encoding='utf-8'?>
<testproblem>
  <name>Nonlinearified linear Helmholtz problem</name>
  <owner userid="wence"/>
  <tags>firedrake</tags>
  <problem_definition length="short" nprocs="1">
    <command_line>mpiexec -n 3 python test_firedrake_parallel_nonlinear_helmholtz.py</command_line>
  </problem_definition>
  <variables>
    <variable name="l2_conv" language="python">
import pickle
with open("test-output.dat", "r") as f:
    l2_conv = pickle.load(f)
    </variable>
  </variables>
  <pass_tests>
    <test name="L2 convergence" language="python">assert (l2_conv &gt; 3.5).all()</test>
  </pass_tests>
  <warn_tests/>
</testproblem>
